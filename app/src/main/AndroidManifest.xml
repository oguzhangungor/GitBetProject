    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GitBetProject">


        <activity android:name="com.ogungor.gitbetproject.loginsignup.LoginSignUpActivity">
        </activity>
        <activity android:name="com.ogungor.gitbetproject.splash.SplashActivity"
            android:theme="@style/Theme.GitBetProject.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.ogungor.gitbetproject.feed.FeedActivity">
       </activity>
        <activity android:name="com.ogungor.gitbetproject.feed.profile.changepswrd.ChangePassword">
        </activity>

        <activity android:name=".loginsignup.signUpFragment.usercontract.UserContractActivity"/>
    </application>

</manifest>